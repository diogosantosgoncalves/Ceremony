<Window x:Class="Ceremony.View.Tela_Tipos_Servicos"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ceremony.View"
        mc:Ignorable="d"
        Title="Tela Tipos Serviços" Height="130" Width="300">
    <StackPanel>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Label Content="Cadastro de Tipos de Serviços"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal" >
            <Label Content="Tipo: " />
            <TextBox x:Name="lb_tipo" Width="220"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0 10 0 0">
            <Button x:Name="bt_Salvar" Content="Salvar" Click="bt_Salvar_Tipo"/>
        </StackPanel>
    </StackPanel>
</Window>
